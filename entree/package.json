{
  "name": "@seasoned/entree",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "entree:dev": "vite",
    "entree:build": "tsc -b && vite build",
    "entree:lint": "eslint .",
    "entree:lint:fix": "eslint . --fix",
    "entree:format": "prettier --write \"{src/**/*,*}.{ts,tsx,js,jsx,json,css,scss,md}\"",
    "entree:format:check": "prettier --check \"{src/**/*,*}.{ts,tsx,js,jsx,json,css,scss,md}\"",
    "entree:preview": "vite preview",
    "entree:test": "vitest",
    "entree:test:watch": "vitest --watch",
    "entree:coverage": "vitest run --coverage",
    "entree:storybook": "storybook dev -p 6006",
    "entree:build-storybook": "storybook build"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.90.12",
    "dexie": "^4.2.1",
    "dexie-react-hooks": "^4.2.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@tanstack/react-query-devtools": "^5.91.1",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "eslint": "^9.18.0",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "globals": "^16.5.0",
    "sass": "^1.84.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.17.0",
    "vite": "npm:rolldown-vite@7.3.0",
    "vite-plugin-pwa": "^1.2.0",
    "vitest": "^4.0.16",
    "@vitest/coverage-v8": "^4.0.16",
    "jsdom": "^27.3.0",
    "@storybook/react": "^8.6.15",
    "@storybook/react-vite": "^8.6.15",
    "@storybook/addon-essentials": "^8.6.15",
    "@storybook/addon-interactions": "^8.6.15",
    "@storybook/addon-links": "^8.6.15",
    "@storybook/blocks": "^8.6.15",
    "@storybook/test": "^8.6.15",
    "storybook": "^8.6.15",
    "prettier": "^3.4.2"
  },
  "pnpm": {
    "overrides": {
      "vite": "npm:rolldown-vite@7.3.0"
    }
  }
}
